{
  "name": "st4r-ui",
  "type": "module",
  "version": "1.0.1",
  "description": "This is a vue3 component library",
  "author": "de4dst4r <de4dst4r@outlook.com>",
  "license": "MIT",
  "keywords": [
    "vue3",
    "components",
    "ui",
    "library"],
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js",
      "require": "./lib/index.js"
    },
    "./es/*": "./es/*",
    "./lib/*": "./lib/*",
    "./dist/*": "./dist/*",
    "./package.json": "./package.json"
  },
  "main": "es/index.js",
  "types": "es/index.d.ts",
  "files": [
    "dist",
    "es",
    "lib"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "vite build",
    "build:umd": "vite build --config vite.umd.config.ts",
    "build:style": "vite build --config vite.style.config.ts"

  },
  "dependencies": {
    "@floating-ui/vue": "^1.0.2",
    "@st4r-ui/utils": "workspace:*",
    "@v-c/utils": "^0.0.26",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12"
  }
}
